#is_prime

def is_prime(n):
    if(n==0 or n==1): 
        return False
    for x in range (2,n):
        if (n % x) == 0:
             return False
    if (x < 0 ):
        return False 
    else: 
        return True
    
print(is_prime(2))


# print_primes 

def print_primes(n):
    for n in range(1,n,-1):
        if is_prime(n) == True:
            print(n)
print_primes(12)
    

